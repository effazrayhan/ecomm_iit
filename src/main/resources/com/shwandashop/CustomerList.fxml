<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="900" prefWidth="1600" style="-fx-background-color: #efe7da;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/24.0.1">
    <children>
        <Label layoutX="750" layoutY="40" style="-fx-font-size: 32px;" text="Customer List" />
        <TableView fx:id="customerTable" layoutX="200" layoutY="120" prefHeight="700" prefWidth="1200">
            <columns>
                <TableColumn prefWidth="300.0" text="Name" />
                <TableColumn prefWidth="450.0" text="Email" />
                <TableColumn prefWidth="450.0" text="Contact Number" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
