<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="900" prefWidth="1600" style="-fx-background-color: #efe7da;" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.shwandashop.AddProductController">
    <children>
        <Label layoutX="680.0" layoutY="100.0" style="-fx-font-size: 32px;" text="Add Product" />
        <TextField fx:id="productName" layoutX="276.0" layoutY="287.0" prefHeight="63.0" prefWidth="1000.0" promptText="Product Name" />
        <Button fx:id="addProductButton" layoutX="315.0" layoutY="645.0" prefHeight="77.0" prefWidth="927.0" text="Add Product">
         <font>
            <Font name="Droid Sans Fallback" size="35.0" />
         </font></Button>
      <TextField fx:id="productId" layoutX="274.0" layoutY="191.0" prefHeight="63.0" prefWidth="1000.0" promptText="Product ID" />
      <TextField fx:id="productQuantity" layoutX="276.0" layoutY="387.0" prefHeight="63.0" prefWidth="1000.0" promptText="Quantity" />
      <TextField fx:id="productPrice" layoutX="276.0" layoutY="487.0" prefHeight="63.0" prefWidth="1000.0" promptText="Price" />
       <Button fx:id="returnButton" layoutX="1454.0" layoutY="53.0" mnemonicParsing="false" onAction="#onReturnClick" text="Return">
            <font>
                <Font name="DejaVuMathTeXGyre-Regular" size="23.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
